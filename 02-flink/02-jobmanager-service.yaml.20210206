apiVersion: v1
kind: Service
metadata:
  name: flink-jobmanager
  namespace: flink
  annotations:
    prometheus.io/scrape: "true"
    prometheus.io/port: "9999"
spec:
  ports:
  - name: rpc
    port: 6123
  - name: blob
    port: 6124
  - name: query
    port: 6125
  - name: ui
    port: 8081
  - name: prom
    port: 9999
  selector:
    app: flink
    component: jobmanager
